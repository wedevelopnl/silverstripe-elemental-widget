{
    "name": "wedevelopnl/silverstripe-elemental-widget",
    "description": "Re-usable widgets for silverstripe-elemental",
    "keywords": [
        "widget",
        "silverstripe",
        "elemental"
    ],
    "type": "silverstripe-vendormodule",
    "require": {
        "php": ">=8.1",
        "silverstripe/framework": "^4.11",
        "silverstripe/cms": "^4.11",
        "dnadesign/silverstripe-elemental": "^4.9",
        "sheadawson/silverstripe-dependentdropdownfield": "^2",
        "unclecheese/display-logic": "^2.0",
        "stevie-mayhew/hasoneedit": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\ElementalWidget\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    }
}
